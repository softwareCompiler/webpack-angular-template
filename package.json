  "engines": {
    "node": "~4.4.x"
  },
  "keywords": [
    "webpack",
    "angular",
    "RxJS",
    "HTML5",
    "service worker",
    "offline",
    "reactive programming",
    "MVC",
    "observer",
    "es6",
